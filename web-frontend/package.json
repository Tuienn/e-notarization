        "@tanstack/react-query": "^5.90.19",
        "@tanstack/react-router": "^1.153.2",
        "axios": "^1.13.2",
        "crypto-js": "^4.2.0",
        "i18next": "^25.8.0",
        "i18next-browser-languagedetector": "^8.2.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-hook-form": "^7.71.1",
        "react-i18next": "^16.5.3",

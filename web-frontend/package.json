        "prettier": "3.8.0",
        "rollup-plugin-visualizer": "^6.0.5",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.4",
        "vite": "npm:rolldown-vite@7.2.5",
        "vite-plugin-svgr": "^4.5.0"
